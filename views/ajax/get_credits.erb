<%
return_message = {}
begin
    service = params['service']
    # return
    return_message[:credits] = BlackStack::I2P::Account.where(:id=>@login.user.id_account).first.credits(service)
    return_message[:status] = 'success'
rescue => e
    return_message[:status] = e.to_console
end
return return_message.to_json
%>